<%= render :partial => 'layouts/form_error_list', :locals => {objeto: tipo_produto} %>

<%= form_with(model: tipo_produto, local: true) do |form| %>
  <div class="row">
    <div class="form-group col-md-12">
      <%= form.label :nome %>
      <%= form.text_field :nome, :class => 'form-control' %>
    </div>
    <div class="form-group col-md-12">
      <%= form.submit :class => "btn btn-primary" %>
    </div>
  </div>
<% end %>